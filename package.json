{
  "name": "client-web-api-sandbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --cache \"src/**/*\"",
    "lint:fix": "eslint --cache \"src/**/*\" --fix",
    "format": "prettier --cache --check \"./src\"",
    "format:fix": "prettier --cache --write \"./src\"",
    "markup": "markuplint \"src/**/*.html\"",
    "markup:fix": "markuplint \"src/**/*.html\" --fix",
    "check": "npm run lint && npm run format && npm run markup",
    "check:fix": "npm run lint:fix && npm run format:fix && npm run markup:fix"
  },
  "dependencies": {
    "@radix-ui/themes": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/dom-mediacapture-transform": "^0.1.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@types/node": "^20.14.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "markuplint": "^4.9.2",
    "prettier": "^3.3.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  }
}
